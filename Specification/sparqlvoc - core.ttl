  dct:conformsTo section:QueryUnit;
  skos:definition "Represents the unit of a query in SPARQL. It is the root node in the SPARQL abstract syntax tree."@en;
  skos:prefLabel "Query Unit"@en;
  rdfs:isDefinedBy sparql:.

